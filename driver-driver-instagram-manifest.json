{
	"manifest-version": 1,

	"name": "instagram-photo-driver",
	"databox-type": "driver",
	"version": "0.5.1",

	"description": "A driver to gather information about your Instagram posts.",
	"author": "Edgar Bodiaj <Edgar.Bodiaj@nottingham.ac.uk>",
	"license": "MIT",
	"tags": [
		"instagram",
		"driver",
		"databox"
	],
	"homepage": "https://github.com/EdgarBodiaj/Databox-Instagram-Driver",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EdgarBodiaj/Databox-Instagram-Driver"
	},
	"external-whitelist": [
		{
			"urls": [
				"https://www.instagram.com",
				"https://i.instagram.com",
				"https://scontent-lhr3-1.cdninstagram.com"
			]

		}
	],
	"resource-requirements": {
		"store": "core-store"
	},
	"provides": [
		{
			"data-source-type": "instagram::photoData",
			"description": "Instagram photo metadata",
			"store-type": "TSBLOB",
			"schema": {}
		}
	]
}
